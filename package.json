{
  "name": "pettube",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/index.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "build:server": "babel src --out-dir build",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start:dev": "nodemon --exec babel-node build/index.js",
    "start": "node build/index.js",
    "prestart": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.4",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local-mongoose": "^6.0.1",
    "path": "^0.12.7",
    "pug": "^3.0.0"
  },
  "engines": {
    "node": "15.3.0"
  }
}
